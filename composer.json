{
  "name": "yuncms/yuncms-news",
  "description": "The news module for the yuncms.",
  "keywords": [
    "yuncms",
    "news"
  ],
  "type": "yii2-extension",
  "license": "MIT",
  "require": {
    "yiisoft/yii2": "~2.0.6",
    "yuncms/yuncms-user": "~2.0"
  },
  "autoload": {
    "psr-4": {
      "yuncms\\news\\": ""
    }
  },
  "extra": {
    "yuncms": {
      "name": "news",
      "i18n": {
        "class": "yii\\i18n\\PhpMessageSource",
        "sourceLanguage": "en-US",
        "basePath": "@yuncms/news/messages"
      },
      "migrationNamespace": "yuncms\\news\\migrations",
      "backend": {
        "class": "yuncms\\news\\backend\\Module"
      },
      "frontend": {
        "class": "yuncms\\news\\frontend\\Module"
      }
    },
    "branch-alias": {
      "dev-master": "2.0-dev"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}
